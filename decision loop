1.triagnle or not:
#include<stdio.h>
int main(){
 int a,b,c;
 scanf("%d %d %d",&a,&b,&c);
 if (a+b>c && b+c>a &&c+a>b){
     printf("It is a triangle");
 }
 else{
     printf("no");
     }
 }

