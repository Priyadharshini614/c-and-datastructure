1.reverser the array:
#include<stdio.h>  
int main()  
{  
    int size, arr[size], i;  
    scanf("%d", &size);  
    for(i =0;i<size;i++)  
    {  
        scanf("%d",&arr[i]);  
    }  
    int rev[size],count= 0;  
    for(i=size-1;i>=0;i--)  
    {  
        rev[count]=arr[i];  
        count++;  
    }  
    for(i=0;i<size;i++)  
    {  
        printf("%d ",rev[i]);  
    }  
}  

2.o's and 1's:
#include<stdio.h>
int main()
{
    int arr[100],size,count=0,index;
    scanf("%d",&size);
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    for(int index=0;index<size;index++)
    {
    if(arr[index]==0)
    count++;
    }
    printf("0:%d 1:%d",count,size-count);
}

3.count the lesser than key element:
#include <stdio.h>

int main()
{
    int arr[100],size,count=0,index,key;
    scanf("%d",&size);
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    scanf("%d",&key);
    for(int index=0;index<size;index++)
    {
        if(arr[index]<key)
        count+=1;
    
}
printf("%d",count);
return 0;
}

4.print repeated element:
#include <stdio.h>
#include<stdio.h>
void main()
{
int size;
scanf("%d ",&size);
int arr[size];
for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);

for(int index=0;index<size;index++)
{
  for (int j=index+1;j<size; j++) 
    {
        if(arr[index]==arr[j])
     printf("%d ",arr[index]);
    }
}
}

5.search the key element:
#include <stdio.h>

int main()
{
    int arr[100],size,index,key,count,flag=0;
    scanf("%d",&size);
    for(index=0;index<size;index++)
    scanf("%d",&arr[index]);
    scanf("%d",&key);
    for(int j=0;j<size;j++)
    {
        if(arr[j]==key){
        flag=0;
        printf("%d",arr[j]);
        }
        else
        flag=1;
    }
    if(flag==1)
    printf("-1");

    return 0;
}

6.segregate 0's and 1's:
#include<stdio.h>  
int main()  
{  
    int size,i;  
    scanf("%d", &size);  
    int arr[size];  
    for(int i=0;i<size;i++)  
        scanf("%d",&arr[i]);  
    for(int i=0;i<size;i++)  
    {  
        int num=arr[i];  
        for(int j=i+1;j<size;j++)  
        {  
            if(arr[j]<num)  
            {  
                int temp = arr[j];  
                arr[j] = arr[i];  
                arr[i] = temp;  
            }  
        }  
    }  
    for(int i=0;i<size;i++)  
    {  
        printf("%d ",arr[i]);  
    }  
}  

7.replace every element in an array with the sum of its right side elements:

#include<stdio.h>
int main()
{
    int size, index,sum=0;
    scanf("%d",&size);
    int arr[size];
    for(index=0;index<size;index++)
{       scanf("%d",&arr[index]);
        sum+=arr[index];
 }   for(index=0;index<size;index++)
    {
        sum-=arr[index];
        arr[index]=sum;
    }

 for(index=0;index<size;index++)
        printf("%d ",arr[index]);
    return 0;
}


8.reverse the array element:
#include<stdio.h>  
int main()  
{  
    int size, arr[size], i;  
    scanf("%d", &size);  
    for(i =0;i<size;i++)  
    {  
        scanf("%d",&arr[i]);  
    }  
    int rev[size],count= 0;  
    for(i=size-1;i>=0;i--)  
    {  
        rev[count]=arr[i];  
        count++;  
    }  
    for(i=0;i<size;i++)  
    {  
        printf("%d ",rev[i]);  
    }  
}  

9.insertion:
#include<stdio.h>
void main()
{
int size,index,arr[index];
scanf("%d ",&size);
for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
   int val,pos;
   scanf("%d %d",&val,&pos);
    for(int index=size;index>=pos;index--)
    arr[index]=arr[index-1];
    arr[pos-1]=val;
    for(int index=0;index<=size;index++)
    printf("%d ",arr[index]);
}

10.deletion:

#include<stdio.h>
void main()
{
int index,arr[index],size,pos;
scanf("%d ",&size);
for(int index=0;index<size;index++)
   scanf("%d",&arr[index]);
   scanf("%d",&pos);
    for(int index=pos-1;index<size-1;index++)
    arr[index]=arr[index+1];
    size--;
    for(int index=0;index<size;index++)
   printf("%d ",arr[index]);
}

