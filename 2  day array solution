1.reverser the array:
#include <stdio.h>

int main()
{
    int arr[1000],size;
    scanf("%d",&size);
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    for(int index=size-1;index>=0;index--)
    printf("%d",arr[index]);
   

    return 0;
}
2.o's and 1's:
#include<stdio.h>
int main()
{
    int arr[100],size,count=0,index;
    scanf("%d",&size);
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    for(int index=0;index<size;index++)
    {
    if(arr[index]==0)
    count++;
    }
    printf("0:%d 1:%d",count,size-count);
}

3.count the lesser than key element:
#include <stdio.h>

int main()
{
    int arr[100],size,count=0,index,key;
    scanf("%d",&size);
    for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
    scanf("%d",&key);
    for(int index=0;index<size;index++)
    {
        if(arr[index]<key)
        count+=1;
    
}
printf("%d",count);
return 0;
}

4.print repeated element:
#include <stdio.h>
#include<stdio.h>
void main()
{
int size;
scanf("%d ",&size);
int arr[size];
for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);

for(int index=0;index<size;index++)
{
  for (int j=index+1;j<size; j++) 
    {
        if(arr[index]==arr[j])
     printf("%d ",arr[index]);
    }
}
}

5.search the key element:
#include <stdio.h>

int main()
{
    int arr[100],size,index,key,count,flag=0;
    scanf("%d",&size);
    for(index=0;index<size;index++)
    scanf("%d",&arr[index]);
    scanf("%d",&key);
    for(int j=0;j<size;j++)
    {
        if(arr[j]==key){
        flag=0;
        printf("%d",arr[j]);
        }
        else
        flag=1;
    }
    if(flag==1)
    printf("-1");

    return 0;
}

6.segregate 0's and 1's:
#include<stdio.h>  
int main()  
{  
    int size,i;  
    scanf("%d", &size);  
    int arr[size];  
    for(int i=0;i<size;i++)  
        scanf("%d",&arr[i]);  
    for(int i=0;i<size;i++)  
    {  
        int num=arr[i];  
        for(int j=i+1;j<size;j++)  
        {  
            if(arr[j]<num)  
            {  
                int temp = arr[j];  
                arr[j] = arr[i];  
                arr[i] = temp;  
            }  
        }  
    }  
    for(int i=0;i<size;i++)  
    {  
        printf("%d ",arr[i]);  
    }  
}  

7.replace every element in an array with the sum of its right side elements:

#include<stdio.h>
int main()
{
    int size, index,sum=0;
    scanf("%d",&size);
    int arr[size];
    for(index=0;index<size;index++)
{       scanf("%d",&arr[index]);
        sum+=arr[index];
 }   for(index=0;index<size;index++)
    {
        sum-=arr[index];
        arr[index]=sum;
    }

 for(index=0;index<size;index++)
        printf("%d ",arr[index]);
    return 0;
}


8.reverse the array element:
#include<stdio.h>  
int main()  
{  
    int size, arr[size], i;  
    scanf("%d", &size);  
    for(i =0;i<size;i++)  
    {  
        scanf("%d",&arr[i]);  
    }  
    int rev[size],count= 0;  
    for(i=size-1;i>=0;i--)  
    {  
        rev[count]=arr[i];  
        count++;  
    }  
    for(i=0;i<size;i++)  
    {  
        printf("%d ",rev[i]);  
    }  
}  

9.insertion:
#include<stdio.h>
void main()
{
int size,index,arr[index];
scanf("%d ",&size);
for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
   int val,pos;
   scanf("%d %d",&val,&pos);
    for(int index=size;index>=pos;index--)
    arr[index]=arr[index-1];
    arr[pos-1]=val;
    for(int index=0;index<=size;index++)
    printf("%d ",arr[index]);
}

10.deletion:

#include<stdio.h>
void main()
{
int index,arr[index],size,pos;
scanf("%d ",&size);
for(int index=0;index<size;index++)
   scanf("%d",&arr[index]);
   scanf("%d",&pos);
    for(int index=pos-1;index<size-1;index++)
    arr[index]=arr[index+1];
    size--;
    for(int index=0;index<size;index++)
   printf("%d ",arr[index]);
}

11.swap the pair of element:
#include<stdio.h>
void main()
{
int size,count,index;
scanf("%d ",&size);
int arr[size];
for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);
for(int start=0;start<size-1;start+=2)
    {
   int temp=arr[start];
    arr[start]=arr[start+1];
    arr[start+1]=temp;
    }
    for(int index=0;index<size;index++)
    printf("%d ",arr[index]);
}

12.rotate element:
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,rot,i,temp,j;
    scanf("%d",&n);
    int a[n];
    scanf("%d",&rot);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
   
    for(j=0;j<rot;j++)
    {
         temp=a[n-1];
        for(i=n-1;i>0;i--)
        {
            a[i]=a[i-1];
        }
        a[0]=temp;
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}


13.sorted array:
#include <stdio.h>

int Sorted(int arr[],int num) {
    int count=0;
    for (int i=0;i<num;i++) {
        if (arr[i]>arr[(i+1)%num]) {
            count++;
        }
    }
    return count <= 1;
}

int main() {
    int num;
    scanf("%d", &num);
    int arr[num];
    for (int i=0;i<num;i++) 
    {
        scanf("%d", &arr[i]);
 if(Sorted(arr, num)) 
    {
    printf("True");
    }
    else
    printf("False");

}
    return 0;
}

14.remove duplicate element:
#include<stdio.h>
void main()
{
int arr[100],index;
int size;
scanf("%d",&size);

for(int index=0;index<size;index++)
    scanf("%d",&arr[index]);


    for(int start=0;start<size-1;start++)
{
   for(int j=start+1;j<size;j++)
{

if (arr[start]==arr[j])
{
for(int j1=j;j1<size;j1++)
arr[j1]=arr[j+1];
size--;
j--;
}
}
}
   for(int index=0;index<size;index++)
   printf("%d ",arr[index]);
}


15.find smallest element:
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int size=5,index;
    int arr[size];
    for(index=0;index<size;index++)
        scanf("%d",&arr[index]);
     for(index=0;index<size;index++)
     {  
        for(int j=index+1;j<size;j++) 
          if(arr[j]<arr[index])  
            {  
                int temp = arr[j];  
                arr[j] = arr[index];  
                arr[index] = temp;
          }
     }
         
    for(index=0;index<2;index++)
    printf("%d ",arr[index]);   
    return 0;
}





